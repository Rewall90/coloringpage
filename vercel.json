{
  "build": {
    "env": {
      "HUGO_VERSION": "0.140.2"
    }
  },
  "buildCommand": "hugo --minify",
  "outputDirectory": "./public",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "noindex, nofollow, noarchive, nosnippet, noimageindex"
        }
      ]
    }
  ]
}