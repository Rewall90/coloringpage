{
  "build": {
    "env": {
      "HUGO_VERSION": "0.140.2"
    }
  },
  "buildCommand": "hugo --minify",
  "outputDirectory": "./public",
  "installCommand": "curl -L -O https://github.com/gohugoio/hugo/releases/download/v${HUGO_VERSION}/hugo_extended_${HUGO_VERSION}_linux-amd64.tar.gz && tar -xzf hugo_extended_${HUGO_VERSION}_linux-amd64.tar.gz && chmod +x hugo"
}